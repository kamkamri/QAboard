<h2>拠点担当者_ログイン</h2>

<%= form_with model: @end_user, url: end_user_session_path do |f| %>
  <div class="field">
    <%= f.label :employee_number, "社員番号" %><br />
    <%= f.text_field :employee_number, autofocus: true, autocomplete: "on" %>
  </div>

  <div class="field">
    <%= f.label :password, "パスワード" %><br />
    <%= f.password_field :password, autocomplete: "current-password" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>

<%= render "public/shared/links" %>
