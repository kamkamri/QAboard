<!--<h1>Admin::Notifications#index</h1>-->
<div class = "container-fluid">
  <div class = "row">
    <div class = "col-md-8">
      <!--通知が存在する場合-->
      <% if @notifications.exists? %>
        <!--部分テンプレート：ジャンル検索-->
        <%= render 'layouts/notification_info', notifications: @notifications %>
      <!--通知が存在しない場合-->
      <% else %>
        <p>通知はありません</p>
      <% end %>
    </div>
  </div>
</div>
